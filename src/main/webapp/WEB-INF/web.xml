<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>EpicWorthCar</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.jsp</welcome-file>
		<welcome-file>default.htm</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>mainFormAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.MainFormAction</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>mainFormAction</servlet-name>
		<url-pattern>/mainFormAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deleteUserAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.DeleteFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteUserAction</servlet-name>
		<url-pattern>/deleteUserAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>joinFormAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.JoinFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinFormAction</servlet-name>
		<url-pattern>/joinFormAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>loginFormAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.LoginFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginFormAction</servlet-name>
		<url-pattern>/loginFormAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logoutAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.LogoutAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutAction</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>updateUserFormAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.UpdateUserFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>updateUserFormAction</servlet-name>
		<url-pattern>/updateFormAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FindUserAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.usermanagement.FindUserAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FindUserAction</servlet-name>
		<url-pattern>/FindUserAction</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>findCarInfoAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.reservation.FindCarInfoAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>findCarInfoAction</servlet-name>
		<url-pattern>/findCarInfoAction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>reserveFormAction</servlet-name>
		<servlet-class>epicworthcar.user.controller.reservation.ReserveFormAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>reserveFormAction</servlet-name>
		<url-pattern>/reserveFormAction</url-pattern>
	</servlet-mapping>


	<!-- JSP files mapping -->

	<servlet>
		<servlet-name>header</servlet-name>
		<jsp-file>/views/module/header.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>header</servlet-name>
		<url-pattern>/header</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>footer</servlet-name>
		<jsp-file>/views/module/footer.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>footer</servlet-name>
		<url-pattern>/footer</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>joinForm</servlet-name>
		<jsp-file>/views/user/joinForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinForm</servlet-name>
		<url-pattern>/join</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>myPage</servlet-name>
		<jsp-file>/views/user/mypage.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>myPage</servlet-name>
		<url-pattern>/mypage</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/views/user/loginForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

