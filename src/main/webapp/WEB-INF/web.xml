	</servlet>
	<servlet-mapping>
		<servlet-name>updateUserForm</servlet-name>
		<url-pattern>/updateUserForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>deleteUserForm</servlet-name>
		<jsp-file>/views/user/deleteUserForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>deleteUserForm</servlet-name>
		<url-pattern>/deleteUserForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>reserveForm</servlet-name>
		<jsp-file>/views/reserve/reserveForm.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>reserveForm</servlet-name>
		<url-pattern>/reserveForm</url-pattern>
	</servlet-mapping>


</web-app>